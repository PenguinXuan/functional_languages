1.2
log
@-
@
text
@# $Id: 33-quadratic.sb,v 1.1 2019-01-18 11:47:25-08 - - $
#
# Quadratic equation solver
#

        print "Quadratic Equation solver."
loop:   print "Input a, b, c"
        input a, b, c
        if eof = 1 goto stop
        let q = sqrt (b ^ 2 - 4 * a * c)
        print "Equation: ", a, " * x ^ 2 +", b, " * x +", c
        print "root1 = ", (- b + q) / (2 * a)
        print "root2 = ", (- b - q) / (2 * a)
        goto loop
stop:
@


1.1
log
@-
@
text
@d1 1
a1 1
# $Id: 33-quadratic.sb,v 1.1 2019-01-17 17:24:45-08 - - $
d10 1
a10 1
        let q = sqrt( b ^ 2 - 4 * a * c )
d12 2
a13 2
        print "root1 = ", ( - b + q ) / ( 2 * a )
        print "root2 = ", ( - b - q ) / ( 2 * a )
@
